{
    "name": "my-boiler-plate",
    "version": "1.0.0",
    "scripts": {
        "build:server": "babel server --out-dir buildServer",
        "client": "cd client && yarn start",
        "server": "nodemon --exec babel-node server/index.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "start": "node buildServer/index.js",
        "heroku-postbuild": "npm run build:server && cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
    },
    "dependencies": {
        "@babel/node": "7.8.7",
        "@babel/polyfill": "7.10.4",
        "@babel/preset-env": "7.10.0",
        "aws-sdk": "2.704.0",
        "bcrypt": "5.0.0",
        "body-parser": "^1.18.3",
        "connect-mongo": "3.2.0",
        "cookie-parser": "1.4.5",
        "coolsms-node-sdk": "0.0.2",
        "cors": "2.8.5",
        "express": "^4.16.4",
        "express-session": "1.17.1",
        "jsonwebtoken": "8.5.1",
        "mongoose": "5.9.15",
        "morgan": "1.10.0",
        "multer": "1.4.2",
        "multer-s3": "2.9.0",
        "nodemailer": "6.4.10",
        "nodemailer-smtp-transport": "2.7.4",
        "passport": "0.4.1",
        "passport-local-mongoose": "6.0.1",
        "passport-naver": "1.0.6"
    },
    "devDependencies": {
        "@babel/cli": "7.10.0",
        "@babel/core": "7.10.0",
        "concurrently": "^4.0.1"
    }
}
